{
  "name": "GreatOwl/OwlPellet",
  "description": "Skeleton setup to make it easier to get up and running on a new project",
  "minimum-stability": "stable",
  "license": "proprietary",
  "authors": [
    {
      "name": "Andrew Nowak",
      "email": "andrew@nowakfamily.net"
    }
  ],
  "require": {
    "php": ">=5.4.0",
    "acclimate/container": "^1.0",
    "monolog/monolog": "^1.13",
    "slim/slim": "^3.0@beta",
    "symfony/config": ">=2.5",
    "symfony/dependency-injection": ">=2.6.3",
    "symfony/yaml": ">=2.5"
  },
  "require-dev": {
    "phpunit/phpunit": "4.*",
    "phpunit/php-invoker": "*",
    "phpmd/phpmd": ">=1.4",
    "squizlabs/php_codesniffer": "*"
  },
  "autoload": {
    "psr-4": {
      "GreatOwl\\Cache\\": "cache",
      "GreatOwl\\Application\\": "src/Application"
    }
  }
}